{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "create-block/hydration-mitosis",
	"version": "0.1.0",
	"title": "Hydration Mitosis",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example of block hydration via a Mitosis compiler.",
	"supports": {
		"html": false
	},
	"attributes": {
		"listName": {
			"type": "string",
			"default": "My TODO list"
		},
		"todos": {
			"type": "array",
			"default": [
				{ "name": "hello", "done": false },
				{ "name": "world", "done": false }
			]
		},
		"maxTodos": {
			"type": "number",
			"default": 5
		},
		"showDone": {
			"type": "boolean",
			"default": true
		}
	},
	"textdomain": "hydration-mitosis",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
